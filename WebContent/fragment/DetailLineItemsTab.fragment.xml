<core:FragmentDefinition xmlns:l="sap.ui.layout" xmlns:cl="sap.ui.commons.layout" xmlns:c="sap.ui.commons" 
    xmlns:t="sap.ui.table"
    xmlns:v="sap.ui.comp.variants"
	xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns="sap.m" xmlns:app="http://schemas.sap.com/sapui5/extension/sap.ui.core.CustomData/1">
		

		<t:Table  class="sapUiSizeCompact" 
		         rows="{currentSalesDocumentLines>/}" 
		         id="lineItemsTable"
				visibleRowCount='5'				
				showColumnVisibilityMenu='true' 
				selectionMode='None'
				enableColumnReordering='true' 
				enableColumnFreeze='true' 
				enableCellFilter='true'
				columnSelect="handleSelectColumn"
				>

				<t:toolbar>
				<Toolbar>	
			<v:VariantManagement id="variantManagement" variantItems="{lineItemVariant>/}" select="handleSelectVariant" enabled="true" manage="handleManageVariant" save="handleSaveVariant" showExecuteOnSelection="false" showShare="false">
			<v:variantItems>
				<v:VariantItem text="{lineItemVariant>text}" key="{lineItemVariant>key}" global="{lineItemVariant>global}" labelReadOnly="{lineItemVariant>readonly}" readOnly="{lineItemVariant>readonly}" lifecyclePackage="{lineItemVariant>lifecyclePackage}" accessOptions="{lineItemVariant>accessOptions}"/>				
			</v:variantItems>
		   </v:VariantManagement>			
				 <ToolbarSpacer/>
					<Button icon="sap-icon://add" text="Add Row" enabled='{currentState>/isEditMode}' visible='{currentState>/isEditMode}'
						press="handleAddDetailLine" />
				</Toolbar>
				</t:toolbar>
		    <t:columns>
 			    <t:Column id='SELECTIONID' width="30px" visible="{currentState>/isEditMode}">
 			    <CheckBox id="SELECTION_CHECK" text="Selection" tooltip="Select All items" selected="false" select="handleToggleSelection" enabled= '{currentState>/isEditMode}' />
 			   <!--  <ToggleButton icon="sap-icon://multi-select" width="15px" class="size1" press="handleToggleSelection"></ToggleButton> -->
			   <!--  <core:Icon  src="sap-icon://multi-select"  width="15px" class="size1" /> --> 			    	
				   	<t:template><CheckBox select="handleDetailOnSelectLines" selected="{path: 'currentSalesDocumentLines>Selected'}" enabled="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}))  }"  /></t:template>
			    </t:Column>
				<t:Column id= "STATUS" width="50px" visible="{= (${currentState>/isSalesOrder})}">
					<Text text="Status"/>
					<t:template>
						<core:Icon src="{= 'sap-icon://' + ((!!${currentSalesDocumentLines>ReasonForRejection}) ? 'message-error' : 'message-success') }" tooltip="{path: 'currentSalesDocumentLines>ReasonForRejection', formatter: 'gdt.salesui.util.Formatter.rejectionDescription'}" color="{= ((!!${currentSalesDocumentLines>ReasonForRejection}) ? 'red' : 'green') }" press="handleClearRejected" enabled="{= !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; !${currentSalesDocumentLines>DeletedFlag} &amp;&amp; ${currentState>/isEditMode} }" visible="{= ((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) === 'Y' || (!!${currentSalesDocumentLines>ReasonForRejection}) ) }"/>

					</t:template>
				</t:Column>
 			    <t:Column id="NOTES" width="40px">
			    	<Text text="Notes" wrapping="false" />
			    	<t:template><core:Icon src="sap-icon://notes" color="{path: 'currentSalesDocumentLines>HasNotesFlag', formatter: 'gdt.salesui.util.Formatter.isEnabled'}" press="handleDetailLineNotes" /></t:template>
			    </t:Column>
			    <t:Column id="LINE" width="56px" sortProperty="SalesDocumentLineID" >
			    	<Text text="Line"  wrapping="false" />
			    	<t:template><Input tooltip="SAP Line: {path: 'currentSalesDocumentLines>SalesDocumentLineID'}  Parent Line: {path: 'currentSalesDocumentLines>ParentLineID'}" valueStateText="Line number must not be empty. Maximum 12 characters." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>StructuredLineID', type : 'sap.ui.model.type.String', constraints: {minLength: 1, maxLength: 12} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }" change="handleChangeLineID" /></t:template>
			    </t:Column>
 			    <t:Column id="REFLN" width="56px" visible="{currentState>/isQuote}">
 			    	<Text text="Ref Ln" tooltip="Reference Line" />
 			    	<t:template><Input valueStateText="Reference Line number must not be empty. Maximum 6 characters." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>CustomerPOLineID', type : 'sap.ui.model.type.String', constraints: {minLength: 1, maxLength: 6} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection} )) }" /></t:template>
 			    </t:Column>
 			    <t:Column id="POLN" width="56px"  visible="{currentState>/isSalesOrder}">
 			    	<Text text="PO Ln" tooltip="Line number from Customer Purchase Order" />
 			    	<t:template><Input valueStateText="PO Line number must not be empty. Maximum 6 characters." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>CustomerPOLineID', type : 'sap.ui.model.type.String', constraints: {minLength: 1, maxLength: 6} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} ) }"  /></t:template>
 			    </t:Column>
			    <t:Column id="QTY" width="56px">
			    	<Text text="Qty" />
 			    	<t:template><Input valueStateText="Qty must be between 1 and 9,999,999,999." change='handleDetailQtyChange' showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>QTY', type : 'sap.ui.model.type.Integer', formatOptions: { groupingEnabled: true, groupingSeparator:',' }, constraints: {minimum: 1, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }" /></t:template>
			    </t:Column>
			   <!-- 	The BELOW LINES ARE COMMENTED AS THESE FIELDS ARE AVAILABLE IN NEW FEATURED Called 'AVAILABLE QUANTITY -->	 
			   <!--  <t:Column id="ORDERED" width="56px" visible="{currentState>/isSubmittedSalesOrder}">
			    	<Text text="Ordered"/>
 			    	<t:template><Input showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>QtyOrdered', type : 'sap.ui.model.type.Integer', formatOptions: { groupingEnabled: true, groupingSeparator:',' }, constraints: {minimum: 1, maximum: 9999999999} }" editable="false"/></t:template>
			    </t:Column>
			    <t:Column id="RECEIVED" width="56px" visible="{currentState>/isSubmittedSalesOrder}">
			    	<Text text="Received"/>
 			    	<t:template><Input showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>QtyReceived', type : 'sap.ui.model.type.Integer', formatOptions: { groupingEnabled: true, groupingSeparator:',' }, constraints: {minimum: 1, maximum: 9999999999} }" editable="false"/></t:template>
			    </t:Column>
			    <t:Column id="SHIPPED" width="56px" visible="{currentState>/isSubmittedSalesOrder}">
			    	<Text text="Shipped"/>
 			    	<t:template><Input showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>QtyShipped', type : 'sap.ui.model.type.Integer', formatOptions: { groupingEnabled: true, groupingSeparator:',' }, constraints: {minimum: 1, maximum: 9999999999} }" editable="false"/></t:template>
			    </t:Column> -->
			    <t:Column id="BILLED" width="56px" visible="{currentState>/isSubmittedSalesOrder}">
			    	<Text text="Billed" />
 			    	<t:template><Input showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>QtyBilled', type : 'sap.ui.model.type.Integer', formatOptions: { groupingEnabled: true, groupingSeparator:',' }, constraints: {minimum: 1, maximum: 9999999999} }" editable="false"/></t:template>
			    </t:Column>
			    <t:Column id="MFR" width="90px">
			    	<Text text="Manufacturer" />
 			    	<t:template><Input app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" tooltip="SAP Manufacturer ID: {path: 'currentSalesDocumentLines>ManufacturerID'}" id="mfrId" value="{path: 'currentSalesDocumentLines>ManufacturerID', formatter: 'gdt.salesui.util.Formatter.mfrName'}" type="Text" showSuggestion="true" filterSuggests="false" startSuggestion="0" suggest="handleSuggestMfr" maxSuggestionWidth="300px" placeholder="Enter Mfr..." enabled="true" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'})  &amp;&amp; ((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y')) }" change="handleChangeMfrID"/></t:template>
			    </t:Column>
				<t:Column id="PARTNO" width="260px" filterProperty="CustomerPartID">
			    	<Text text="Part # Requested"/>
 			    	<t:template><Input app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" tooltip="SAP Material ID: {path: 'currentSalesDocumentLines>CustomerMaterialID'}" id= "MfrPartID" valueStateText="Requested Part # must not be empty. Maximum 40 characters."  showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>CustomerPartID', type : 'sap.ui.model.type.String', constraints: {minLength: 1, maxLength: 40} }" placeholder="Part # requested..." showValueHelp="true" valueHelpRequest="handlePartIDHelp" change="handleChangePartID" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; ((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y')) }"/></t:template>
			    </t:Column>
			    <t:Column id="DESCRIPTION" width="260px">
			    	<Text text="Description" />
 			    	<t:template><Input value="{currentSalesDocumentLines>Description}" tooltip="{path: 'currentSalesDocumentLines>LongDescription'}" placeholder="Line item description ..." editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }" /></t:template>
			    </t:Column>
				<t:Column id="VENDOR" width="90px">
 		    		<Text text="Vendor" />
 			    	<t:template><Input app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" tooltip="SAP Vendor ID: {path: 'currentSalesDocumentLines>VendorID'}" value="{path: 'currentSalesDocumentLines>VendorID', formatter: 'gdt.salesui.util.Formatter.vdrName'}" type="Text" showSuggestion="true" filterSuggests="false" startSuggestion="0" suggest="handleSuggestMfr" maxSuggestionWidth="300px" placeholder="Enter Vendor..." enabled="true" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"  change="handleChangeMfrID"/></t:template>
 		    	</t:Column>
 			    <t:Column id="LISTPRICE" width="90px">
 			    	<Text text="List Price" />
 			    	<t:template><Input valueStateText="List Price must be between 0 and 9,999,999,999.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>ListPrice', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 10, minFractionDigits: 2, maxFractionDigits: 2}, constraints: {minimum: -9999999999, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }" /></t:template>
 			    </t:Column>
			    <t:Column id="GDTDISCOUNTPERCENT" width="55px">
				    <Text text="GDT Disc. %" />
 			    	<t:template><Input class="gdt_discount" valueStateText="GDT Discount Points must be between 0.00 and 99.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>GDTDiscountPercent', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 2, maxIntegerDigits: 3, minFractionDigits: 2, maxFractionDigits: 3}, constraints: {minimum: 0, maximum: 100} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }" /></t:template>
			   </t:Column>
			    <t:Column id="UNITCOST" width="80px">
			    	<Text text="Unit Cost" />
 			    	<t:template><Input valueStateText="Unit Cost must be between 0 and 9,999,999,999.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>UnitCost', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 10, minFractionDigits: 2, maxFractionDigits: 2}, constraints: {minimum: 0, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"  /></t:template>
			    </t:Column>
			    <t:Column id="EXTENDEDCOST" width="100px">
			    	<Text text="Ext. Cost" />
 			    	<t:template><Input valueStateText="Extended Cost must be between 0 and 9,999,999,999.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>ExtendedCost', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 10, minFractionDigits: 2, maxFractionDigits: 2}, constraints: {minimum: 0, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"  /></t:template>
			    </t:Column>
			    <t:Column id="CUSTOMERDISCOUNT" width="55px">
			    	<Text text="Cust. Disc. %" />
 			    	<t:template><Input class="cust_discount" valueStateText="Customer Discount Points must be between 0.00 and 99.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>CustomerDiscountPercent', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 2, maxIntegerDigits: 3, minFractionDigits: 2, maxFractionDigits: 3}, constraints: {minimum: 0, maximum: 100} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode}  &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"/></t:template>
			    </t:Column>
			    <t:Column id="UNITPRICE" width="80px">
			    	<Text text="Unit Price" />
 			    	<t:template><Input valueStateText="Unit Price must be between 0 and 9,999,999,999.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>UnitPrice', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 10, minFractionDigits: 2, maxFractionDigits: 2}, constraints: {minimum: 0, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode}  &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"/></t:template>
			    </t:Column>
			    <t:Column id="EXTPRICE" width="100px">
			    	<Text text="Ext. Price" />
 			    	<t:template><Input valueStateText="Extended Price must be between 0 and 9,999,999,999.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>ExtendedPrice', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 10, minFractionDigits: 2, maxFractionDigits: 2}, constraints: {minimum: 0, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"  /></t:template>
			    </t:Column>
			    <t:Column id="GP" width="100px">
			    	<Text text="GP" />
 			    	<t:template><Input valueStateText="Gross Profit must be between  0 and 9,999,999,999.99." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>GrossProfit', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 10, minFractionDigits: 2, maxFractionDigits: 2}, constraints: {minimum: 0, maximum: 9999999999} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"  /></t:template>
			    </t:Column>
			    <t:Column id="GP_PERCENT" width="55px">
			    	<Text text="GP %" />
 			    	<t:template><Input valueStateText="Gross Profit % must be between 0.00 and 100." showValueStateMessage="false" value="{path: 'currentSalesDocumentLines>GrossProfitPercentage', type : 'sap.ui.model.type.Float', formatOptions: {decimalSeparator:'.', groupingSeparator:',', minIntegerDigits: 1, maxIntegerDigits: 3, minFractionDigits: 2, maxFractionDigits: 3}, constraints: {minimum: 0, maximum: 100} }" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"  /></t:template>
			    </t:Column>
			    <t:Column  id="STAGE" width="60px">
			        <CheckBox id="STAGE_CHECK" text="Stage" enabled="{= (${currentState>/isEditMode})}" tooltip="Staging required in warehouse process" selected="false" select="handleStagingSelectAll" />
			    	<!-- <Text text="Stage" tooltip="Staging required in warehouse process" /> -->
 			    	<t:template><CheckBox select="handleStagingSelect" tooltip="SAP ItemCategory: {path: 'currentSalesDocumentLines>ItemCategory'}" app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" selected="{path: 'currentSalesDocumentLines>ItemCategory', formatter: 'gdt.salesui.util.Formatter.itemCategoryToStaging'}" enabled="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; ((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') &amp;&amp; ((${currentSalesDocumentLines>EDelivery}  !== 'EDELIVERY')) &amp;&amp; (${currentSalesDocumentLines>MaterialGroup} === 'Z2')) }" /></t:template>
			    </t:Column> 
			    <!--  visible="{currentState>/isSalesOrder}" -->
			    <t:Column id="DROPSHIP" width="55px">
			        <CheckBox id="DROPSHIP_CHECK" text="D/S" enabled="{= (${currentState>/isEditMode})}" tooltip="Drop Ship from vendor to customer" selected="false" select="handleDropShipSelectAll" />
			    	<!-- <Text text="D/S" tooltip="Drop Ship from vendor to customer" /> -->
 			    	<t:template><CheckBox select="handleDropShipSelect" tooltip="SAP ItemCategory: {path: 'currentSalesDocumentLines>ItemCategory'}" app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" selected="{path: 'currentSalesDocumentLines>ItemCategory', formatter: 'gdt.salesui.util.Formatter.itemCategoryToDropShip'}" enabled="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; ((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') &amp;&amp; ((${currentSalesDocumentLines>EDelivery}  !== 'EDELIVERY')) &amp;&amp; (${currentSalesDocumentLines>MaterialGroup}  === 'Z2')) }" /></t:template>
			    </t:Column>
			    <t:Column id="SHIPTO" width="260px">
			    	<Text text="Ship To" />
 			    	<t:template><Input app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" tooltip="SAP ShipTo ID: {path: 'currentSalesDocumentLines>ShipToID'}" id="shipTo" value="{ path: 'currentSalesDocumentLines>ShipToID', formatter: 'gdt.salesui.util.Formatter.shipTo'}" type="Text" showSuggestion="true" filterSuggests="false" startSuggestion="0" suggest="handleSuggestPartner" maxSuggestionWidth="400px" placeholder="Enter Ship To..." enabled="true" editable="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection} || ('' + ${currentSalesDocumentLines>ItemCategory} !== 'YB1')) ) }"   change="handleChangeDetailLineShipTo"/></t:template>
			    </t:Column>
			    <t:Column id="VIA" width="120px">
			    	<Text text="Via" tooltip="Send to customer address via this one-time address"  />
 			    	<t:template><Button type="{path: 'currentSalesDocumentLines>', formatter: 'gdt.salesui.util.Formatter.viaColor'}" text="{path: 'currentSalesDocumentLines>', formatter: 'gdt.salesui.util.Formatter.via'}" tooltip="{path: 'currentSalesDocumentLines>', formatter: 'gdt.salesui.util.Formatter.viatt'}"  press="handleViaDetailPress" app:rowid="{currentSalesDocumentLines>SalesDocumentLineID}" enabled="{= (!${currentSalesDocumentLines>WBSElement} &amp;&amp; !${currentSalesDocumentLines>MarkedAsDeleted} &amp;&amp; ${currentState>/isEditMode} &amp;&amp; (${path:'currentSalesDocumentLines>ItemCategory',formatter:'gdt.salesui.util.Formatter.isEditableOnItemCategory'}) &amp;&amp; (((''+${currentSalesDocumentLines>ItemCategory}).substring(0,1) !== 'Y') || !!${currentSalesDocumentLines>ReasonForRejection}) ) }"   width="100%" /></t:template>
			    </t:Column>
                <t:Column id="SHIPTOATTN" width="90px">
